{
  "$schema": "vscode://schemas/color-theme",
  "name": "Turbo Light",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#fafbfc",
    "editor.foreground": "#1a202c",
    "editorLineNumber.foreground": "#94a3b8",
    "editorLineNumber.activeForeground": "#0a7c72",
    "editor.selectionBackground": "#d1f4ed",
    "editor.selectionHighlightBackground": "#d1f4ed66",
    "editor.lineHighlightBackground": "#f1f5f9",
    "editorCursor.foreground": "#0a7c72",
    "editorWhitespace.foreground": "#e2e8f0",
    "editorIndentGuide.background1": "#e2e8f0",
    "editorIndentGuide.activeBackground1": "#0a7c72",
    "editor.findMatchBackground": "#d1f4ed",
    "editor.findMatchHighlightBackground": "#d1f4ed66",
    "editorBracketMatch.background": "#d1f4ed",
    "editorBracketMatch.border": "#0a7c72",
    "editorBracketHighlight.foreground1": "#1a202c",
    "editorBracketHighlight.foreground2": "#1a202c",
    "editorBracketHighlight.foreground3": "#1a202c",
    "editorBracketHighlight.foreground4": "#1a202c",
    "editorBracketHighlight.foreground5": "#1a202c",
    "editorBracketHighlight.foreground6": "#1a202c",
    "editorBracketPairGuide.activeBackground1": "#1a202c",
    "editorBracketPairGuide.activeBackground2": "#1a202c",
    "editorBracketPairGuide.activeBackground3": "#1a202c",
    "editorBracketPairGuide.activeBackground4": "#1a202c",
    "editorBracketPairGuide.activeBackground5": "#1a202c",
    "editorBracketPairGuide.activeBackground6": "#1a202c",
    "editorBracketPairGuide.background1": "#94a3b8",
    "editorBracketPairGuide.background2": "#94a3b8",
    "editorBracketPairGuide.background3": "#94a3b8",
    "editorBracketPairGuide.background4": "#94a3b8",
    "editorBracketPairGuide.background5": "#94a3b8",
    "editorBracketPairGuide.background6": "#94a3b8",
    "editor.wordHighlightBackground": "#d1f4ed66",
    "editor.wordHighlightStrongBackground": "#d1f4ed66",
    "editorGutter.background": "#fafbfc",
    "editorGutter.modifiedBackground": "#b8845a",
    "editorGutter.addedBackground": "#0a7c72",
    "editorGutter.deletedBackground": "#d63948",
    "editorError.foreground": "#d63948",
    "editorWarning.foreground": "#b8845a",
    "editorInfo.foreground": "#0f9b8e",
    "editorHint.foreground": "#0a7c72",
    "statusBar.background": "#f7fafc",
    "statusBar.foreground": "#1a202c",
    "statusBar.border": "#e2e8f0",
    "statusBar.noFolderBackground": "#f7fafc",
    "statusBar.debuggingBackground": "#f7fafc",
    "statusBarItem.prominentBackground": "#d1f4ed",
    "statusBarItem.prominentForeground": "#0E9C7B",
    "activityBar.background": "#f7fafc",
    "activityBar.foreground": "#1a202c",
    "activityBar.border": "#cbd5e1",
    "activityBar.activeBorder": "#0E9C7B",
    "activityBarBadge.background": "#0E9C7B",
    "sideBar.background": "#f7fafc",
    "sideBar.foreground": "#1a202c",
    "sideBar.border": "#cbd5e1",
    "sideBarTitle.foreground": "#0E9C7B",
    "sideBarSectionHeader.background": "#f1f5f9",
    "sideBarSectionHeader.foreground": "#1a202c",
    "list.activeSelectionBackground": "#e0f2f1",
    "list.activeSelectionForeground": "#1a202c",
    "list.inactiveSelectionBackground": "#f1f5f9",
    "list.hoverBackground": "#f1f5f9",
    "tab.activeBackground": "#ffffff",
    "tab.inactiveBackground": "#f1f5f9",
    "tab.activeForeground": "#1a202c",
    "tab.activeBorder": "#0E9C7B",
    "tab.activeBorderTop": "#0E9C7B",
    "tab.inactiveForeground": "#94a3b8",
    "tab.border": "#e2e8f0",
    "titleBar.activeBackground": "#ffffff",
    "titleBar.activeForeground": "#1a202c",
    "titleBar.border": "#e2e8f0",
    "titleBar.inactiveBackground": "#f1f5f9",
    "titleBar.inactiveForeground": "#94a3b8",
    "panel.background": "#ffffff",
    "panel.border": "#cbd5e1",
    "panelTitle.activeForeground": "#0E9C7B",
    "panelTitle.activeBorder": "#0E9C7B",
    "panelTitle.inactiveForeground": "#A9AAB1",
    "input.background": "#ffffff",
    "input.foreground": "#1a202c",
    "input.border": "#cbd5e1",
    "inputOption.activeBorder": "#0a7c72",
    "dropdown.background": "#ffffff",
    "dropdown.foreground": "#1a202c",
    "dropdown.border": "#e2e8f0",
    "button.background": "#0a7c72",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#0f9b8e",
    "scrollbarSlider.background": "#e2e8f0",
    "scrollbarSlider.hoverBackground": "#94a3b8",
    "scrollbarSlider.activeBackground": "#0a7c72",
    "badge.background": "#0a7c72",
    "badge.foreground": "#ffffff",
    "progressBar.background": "#0a7c72",
    "textLink.foreground": "#0f9b8e",
    "textLink.activeForeground": "#0a7c72",
    "textBlockQuote.background": "#f1f5f9",
    "textBlockQuote.border": "#e2e8f0",
    "textCodeBlock.background": "#f1f5f9",
    "textPreformat.foreground": "#1a202c",
    "textPreformat.background": "#f1f5f9",
    "editorGroup.border": "#cbd5e1",
    "breadcrumb.background": "#fafbfc",
    "breadcrumb.foreground": "#64748b",
    "breadcrumb.focusForeground": "#0a7c72",
    "breadcrumb.activeSelectionForeground": "#0a7c72"
  },
  "semanticTokenColors": {
    "property": "#b8845a",
    "property.readonly": "#b8845a",
    "*.attribute": "#b8845a",
    "namespace.attribute": "#b8845a",
    "module.attribute": "#b8845a",
    "module": "#0a7c72",
    "type": "#A64D79",
    "class": "#2563A8",
    "class.builtin": "#A64D79",
    "class.defaultLibrary": "#A64D79",
    "interface": "#2563A8",
    "enum": "#2563A8",
    "typeParameter": "#2563A8",
    "function": "#b8845a",
    "method": "#b8845a",
    "variable": "#1a202c",
    "parameter": "#1a202c",
    "parameter.unused": "#64748b",
    "*.unused": "#64748b"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#64748b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python builtin types - highest priority",
      "scope": [
        "source.python meta.function.parameters.python support.type.python",
        "source.python meta.function.python support.type.python",
        "source.python support.type.python",
        "meta.function.parameters.python support.type.python",
        "meta.function.python support.type.python",
        "support.type.python"
      ],
      "settings": {
        "foreground": "#A64D79"
      }
    },
    {
      "name": "Function definition keyword (def, function, etc.)",
      "scope": [
        "storage.type.function",
        "storage.type.function.python",
        "storage.type.function.js",
        "storage.type.function.ts"
      ],
      "settings": {
        "foreground": "#2563A8"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#b8845a"
      }
    },
    {
      "name": "String quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string",
        "string.quoted.single punctuation.definition.string",
        "string.quoted.double punctuation.definition.string",
        "string.quoted.triple punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#0f9b8e"
      }
    },
    {
      "name": "Strings in function call arguments",
      "scope": [
        "meta.function-call.arguments.python string.quoted.single.python",
        "meta.function-call.arguments.python string.quoted.double.python",
        "meta.function-call.python meta.function-call.arguments.python string.quoted.single.python",
        "meta.function-call.python meta.function-call.arguments.python string.quoted.double.python",
        "source.python meta.function-call.arguments.python string.quoted.single.python",
        "source.python meta.function-call.arguments.python string.quoted.double.python"
      ],
      "settings": {
        "foreground": "#0f9b8e"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "string.template",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#0a7c72"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "string.quoted.double.json punctuation.definition.string.json",
        "string.quoted.double.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#b8845a"
      }
    },
    {
      "name": "JSON String Value",
      "scope": [
        "source.json string.quoted.double",
        "source.json meta.structure.dictionary.value string.quoted.double"
      ],
      "settings": {
        "foreground": "#0a7c72"
      }
    },
    {
      "name": "YAML Key",
      "scope": [
        "entity.name.tag.yaml",
        "source.yaml entity.name.tag"
      ],
      "settings": {
        "foreground": "#b8845a"
      }
    },
    {
      "name": "YAML String",
      "scope": [
        "source.yaml string.quoted",
        "source.yaml string.unquoted"
      ],
      "settings": {
        "foreground": "#0a7c72"
      }
    },
    {
      "name": "Python type hints",
      "scope": [
        "source.python entity.name.type",
        "source.python storage.type",
        "entity.name.type.python"
      ],
      "settings": {
        "foreground": "#A64D79"
      }
    },
    {
      "name": "TypeScript/JavaScript Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "entity.name.type.alias",
        "entity.name.type.interface",
        "entity.name.type.enum"
      ],
      "settings": {
        "foreground": "#2563A8"
      }
    },
    {
      "name": "TypeScript Interface/Type Keyword",
      "scope": [
        "storage.type.typescript",
        "storage.type.interface.typescript",
        "storage.type.type.typescript"
      ],
      "settings": {
        "foreground": "#b8845a"
      }
    },
    {
      "name": "Function/Method Call",
      "scope": [
        "meta.function-call entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#b8845a"
      }
    },
    {
      "name": "Function/Method",
      "scope": [
        "entity.name.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#b8845a"
      }
    },
    {
      "name": "Function Definition",
      "scope": [
        "entity.name.function.definition",
        "meta.function.definition entity.name.function"
      ],
      "settings": {
        "foreground": "#1a202c"
      }
    },
    {
      "name": "Function parameters",
      "scope": [
        "variable.parameter",
        "variable.parameter.function",
        "variable.parameter.function.language.python",
        "meta.function.parameters.python variable.parameter",
        "meta.function.python variable.parameter"
      ],
      "settings": {
        "foreground": "#1a202c"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#334155"
      }
    },
    {
      "name": "Property/Attribute",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "entity.name.attribute",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#b8845a"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#0f9b8e"
      }
    },
    {
      "name": "Python format placeholders",
      "scope": [
        "constant.character.format.placeholder.other.python",
        "constant.character.format.placeholder.python",
        "meta.format.brace.python constant.character.format.placeholder",
        "string.quoted.single.python constant.character.format.placeholder",
        "string.quoted.double.python constant.character.format.placeholder",
        "meta.function-call.arguments.python constant.character.format.placeholder"
      ],
      "settings": {
        "foreground": "#0f9b8e"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.character",
        "constant.other",
        "constant.language"
      ],
      "settings": {
        "foreground": "#0a7c72"
      }
    },
    {
      "name": "Function call argument punctuation",
      "scope": [
        "source.python meta.function-call.python meta.function-call.arguments.python meta.member.access.python punctuation.definition.arguments.end.python",
        "source.python meta.function-call.python meta.function-call.arguments.python punctuation.definition.arguments.end.python",
        "source.python meta.function-call.python meta.function-call.arguments.python punctuation.definition.arguments.begin.python",
        "source.python meta.function-call.python meta.member.access.python punctuation.definition.arguments.end.python",
        "source.python meta.function-call.python punctuation.definition.arguments.end.python",
        "source.python meta.function-call.python punctuation.definition.arguments.begin.python",
        "meta.function-call.python meta.function-call.arguments.python meta.member.access.python punctuation.definition.arguments.end.python",
        "meta.function-call.python meta.function-call.arguments.python punctuation.definition.arguments.end.python",
        "meta.function-call.python meta.function-call.arguments.python punctuation.definition.arguments.begin.python",
        "meta.function-call.python meta.member.access.python punctuation.definition.arguments.end.python",
        "meta.function-call.python punctuation.definition.arguments.end.python",
        "meta.function-call.python punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python",
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end",
        "punctuation.definition.arguments.begin",
        "punctuation.definition.arguments"
      ],
      "settings": {
        "foreground": "#1a202c"
      }
    },
    {
      "name": "Colon",
      "scope": [
        "punctuation.separator.colon",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#1a202c"
      }
    },
    {
      "name": "Parentheses",
      "scope": [
        "punctuation.section.parens",
        "punctuation.section.parens.begin",
        "punctuation.section.parens.end",
        "meta.group"
      ],
      "settings": {
        "foreground": "#1a202c"
      }
    },
    {
      "name": "Braces",
      "scope": [
        "punctuation.section.braces",
        "punctuation.section.braces.begin",
        "punctuation.section.braces.end",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#1a202c"
      }
    },
    {
      "name": "Operator and Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.section.brackets",
        "punctuation.section.brackets.begin",
        "punctuation.section.brackets.end",
        "punctuation.terminator",
        "punctuation.definition.parameters",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation",
        "punctuation.definition",
        "punctuation.section",
        "meta.bracket"
      ],
      "settings": {
        "foreground": "#1a202c"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#b8845a"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "punctuation.definition.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#0a7c72"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.export",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#2563A8"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#0a7c72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#1a202c"
      }
    }
  ]
}