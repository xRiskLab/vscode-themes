{
  "$schema": "vscode://schemas/color-theme",
  "name": "Turbo Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#1a1f2e",
    "editor.foreground": "#e8eaed",
    "editorLineNumber.foreground": "#5a6578",
    "editorLineNumber.activeForeground": "#10a37f",
    "editor.selectionBackground": "#2d3447",
    "editor.selectionHighlightBackground": "#2d344766",
    "editor.lineHighlightBackground": "#222836",
    "editorCursor.foreground": "#10a37f",
    "editorWhitespace.foreground": "#2d3447",
    "editorIndentGuide.background1": "#2d3447",
    "editorIndentGuide.activeBackground1": "#10a37f",
    "editor.findMatchBackground": "#2d344766",
    "editor.findMatchHighlightBackground": "#2d344766",
    "editorBracketMatch.background": "#2d344766",
    "editorBracketMatch.border": "#10a37f",
    "editorBracketHighlight.foreground1": "#e8eaed",
    "editorBracketHighlight.foreground2": "#e8eaed",
    "editorBracketHighlight.foreground3": "#e8eaed",
    "editorBracketHighlight.foreground4": "#e8eaed",
    "editorBracketHighlight.foreground5": "#e8eaed",
    "editorBracketHighlight.foreground6": "#e8eaed",
    "editorBracketPairGuide.activeBackground1": "#e8eaed",
    "editorBracketPairGuide.activeBackground2": "#e8eaed",
    "editorBracketPairGuide.activeBackground3": "#e8eaed",
    "editorBracketPairGuide.activeBackground4": "#e8eaed",
    "editorBracketPairGuide.activeBackground5": "#e8eaed",
    "editorBracketPairGuide.activeBackground6": "#e8eaed",
    "editorBracketPairGuide.background1": "#5a6578",
    "editorBracketPairGuide.background2": "#5a6578",
    "editorBracketPairGuide.background3": "#5a6578",
    "editorBracketPairGuide.background4": "#5a6578",
    "editorBracketPairGuide.background5": "#5a6578",
    "editorBracketPairGuide.background6": "#5a6578",
    "editor.wordHighlightBackground": "#2d344766",
    "editor.wordHighlightStrongBackground": "#2d344766",
    "editorGutter.background": "#1a1f2e",
    "editorGutter.modifiedBackground": "#d4a574",
    "editorGutter.addedBackground": "#10a37f",
    "editorGutter.deletedBackground": "#e16d76",
    "editorError.foreground": "#e16d76",
    "editorWarning.foreground": "#d4a574",
    "editorInfo.foreground": "#14b8a6",
    "editorHint.foreground": "#10a37f",
    "statusBar.background": "#222836",
    "statusBar.foreground": "#e8eaed",
    "statusBar.noFolderBackground": "#222836",
    "statusBar.debuggingBackground": "#222836",
    "statusBarItem.prominentBackground": "#2d3447",
    "statusBarItem.prominentForeground": "#56B7A8",
    "activityBar.background": "#1a1f2e",
    "activityBar.foreground": "#e8eaed",
    "activityBarBadge.background": "#0E9C7B",
    "sideBar.background": "#1a1f2e",
    "sideBar.foreground": "#e8eaed",
    "sideBarTitle.foreground": "#56B7A8",
    "sideBarSectionHeader.background": "#222836",
    "sideBarSectionHeader.foreground": "#e8eaed",
    "list.activeSelectionBackground": "#2d3447",
    "list.activeSelectionForeground": "#e8eaed",
    "list.inactiveSelectionBackground": "#222836",
    "list.hoverBackground": "#222836",
    "tab.activeBackground": "#1a1f2e",
    "tab.inactiveBackground": "#222836",
    "tab.activeForeground": "#e8eaed",
    "tab.inactiveForeground": "#5a6578",
    "tab.border": "#222836",
    "titleBar.activeBackground": "#222836",
    "titleBar.activeForeground": "#e8eaed",
    "titleBar.inactiveBackground": "#222836",
    "titleBar.inactiveForeground": "#5a6578",
    "panel.background": "#1a1f2e",
    "panel.border": "#2d3447",
    "panelTitle.activeForeground": "#56B7A8",
    "panelTitle.inactiveForeground": "#A9AAB1",
    "input.background": "#222836",
    "input.foreground": "#e8eaed",
    "input.border": "#2d3447",
    "inputOption.activeBorder": "#10a37f",
    "dropdown.background": "#222836",
    "dropdown.foreground": "#e8eaed",
    "dropdown.border": "#2d3447",
    "button.background": "#10a37f",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#14b8a6",
    "scrollbarSlider.background": "#2d3447",
    "scrollbarSlider.hoverBackground": "#5a6578",
    "scrollbarSlider.activeBackground": "#10a37f",
    "badge.background": "#10a37f",
    "badge.foreground": "#ffffff",
    "progressBar.background": "#10a37f",
    "textLink.foreground": "#14b8a6",
    "textLink.activeForeground": "#10a37f",
    "textBlockQuote.background": "#222836",
    "textBlockQuote.border": "#2d3447",
    "textCodeBlock.background": "#222836",
    "textPreformat.foreground": "#e8eaed",
    "textPreformat.background": "#222836",
    "editorGroup.border": "#2d3447",
    "editorGroup.emptyBackground": "#1a1f2e",
    "editorGroupHeader.tabsBackground": "#1a1f2e"
  },
  "semanticTokenColors": {
    "property": "#d4a574",
    "property.readonly": "#d4a574",
    "*.attribute": "#d4a574",
    "namespace.attribute": "#d4a574",
    "module.attribute": "#d4a574",
    "module": "#14b8a6",
    "type": "#C98A8E",
    "class": "#519BD9",
    "class.builtin": "#C98A8E",
    "class.defaultLibrary": "#C98A8E",
    "interface": "#519BD9",
    "enum": "#519BD9",
    "typeParameter": "#519BD9",
    "function": "#d4a574",
    "method": "#d4a574",
    "variable": "#e8eaed",
    "parameter": "#e8eaed",
    "parameter.unused": "#747485",
    "*.unused": "#747485"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#747485",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python builtin types - highest priority",
      "scope": [
        "source.python meta.function.parameters.python support.type.python",
        "source.python meta.function.python support.type.python",
        "source.python support.type.python",
        "meta.function.parameters.python support.type.python",
        "meta.function.python support.type.python",
        "support.type.python"
      ],
      "settings": {
        "foreground": "#C98A8E"
      }
    },
    {
      "name": "Function definition keyword (def, function, etc.)",
      "scope": [
        "storage.type.function",
        "storage.type.function.python",
        "storage.type.function.js",
        "storage.type.function.ts"
      ],
      "settings": {
        "foreground": "#519BD9"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "String quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string",
        "string.quoted.single punctuation.definition.string",
        "string.quoted.double punctuation.definition.string",
        "string.quoted.triple punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#14b8a6"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "string.template",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#14b8a6"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "string.quoted.double.json punctuation.definition.string.json",
        "string.quoted.double.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "JSON String Value",
      "scope": [
        "source.json string.quoted.double",
        "source.json meta.structure.dictionary.value string.quoted.double"
      ],
      "settings": {
        "foreground": "#14b8a6"
      }
    },
    {
      "name": "YAML Key",
      "scope": [
        "entity.name.tag.yaml",
        "source.yaml entity.name.tag"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "YAML String",
      "scope": [
        "source.yaml string.quoted",
        "source.yaml string.unquoted"
      ],
      "settings": {
        "foreground": "#14b8a6"
      }
    },
    {
      "name": "Python type hints",
      "scope": [
        "source.python entity.name.type",
        "source.python storage.type",
        "entity.name.type.python"
      ],
      "settings": {
        "foreground": "#C98A8E"
      }
    },
    {
      "name": "TypeScript/JavaScript Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "entity.name.type.alias",
        "entity.name.type.interface",
        "entity.name.type.enum"
      ],
      "settings": {
        "foreground": "#519BD9"
      }
    },
    {
      "name": "TypeScript Interface/Type Keyword",
      "scope": [
        "storage.type.typescript",
        "storage.type.interface.typescript",
        "storage.type.type.typescript"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "Function/Method Call",
      "scope": [
        "meta.function-call entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "Function/Method",
      "scope": [
        "entity.name.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "Function Definition",
      "scope": [
        "entity.name.function.definition",
        "meta.function.definition entity.name.function"
      ],
      "settings": {
        "foreground": "#e8eaed"
      }
    },
    {
      "name": "Function parameters",
      "scope": [
        "variable.parameter",
        "variable.parameter.function",
        "variable.parameter.function.language.python",
        "meta.function.parameters.python variable.parameter",
        "meta.function.python variable.parameter"
      ],
      "settings": {
        "foreground": "#e8eaed"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#cbd5e0"
      }
    },
    {
      "name": "Property/Attribute",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "entity.name.attribute",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#14B8A6"
      }
    },
    {
      "name": "Python format placeholders",
      "scope": [
        "constant.character.format.placeholder.other.python",
        "constant.character.format.placeholder.python",
        "meta.format.brace.python constant.character.format.placeholder",
        "string.quoted.single.python constant.character.format.placeholder",
        "string.quoted.double.python constant.character.format.placeholder",
        "meta.function-call.arguments.python constant.character.format.placeholder"
      ],
      "settings": {
        "foreground": "#14B8A6"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.character",
        "constant.other",
        "constant.language"
      ],
      "settings": {
        "foreground": "#10a37f"
      }
    },
    {
      "name": "Function call argument punctuation - white",
      "scope": [
        "source.python meta.function-call.python meta.function-call.arguments.python meta.member.access.python punctuation.definition.arguments.end.python",
        "source.python meta.function-call.python meta.function-call.arguments.python punctuation.definition.arguments.end.python",
        "source.python meta.function-call.python meta.function-call.arguments.python punctuation.definition.arguments.begin.python",
        "source.python meta.function-call.python meta.member.access.python punctuation.definition.arguments.end.python",
        "source.python meta.function-call.python punctuation.definition.arguments.end.python",
        "source.python meta.function-call.python punctuation.definition.arguments.begin.python",
        "meta.function-call.python meta.function-call.arguments.python meta.member.access.python punctuation.definition.arguments.end.python",
        "meta.function-call.python meta.function-call.arguments.python punctuation.definition.arguments.end.python",
        "meta.function-call.python meta.function-call.arguments.python punctuation.definition.arguments.begin.python",
        "meta.function-call.python meta.member.access.python punctuation.definition.arguments.end.python",
        "meta.function-call.python punctuation.definition.arguments.end.python",
        "meta.function-call.python punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python",
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end",
        "punctuation.definition.arguments.begin",
        "punctuation.definition.arguments"
      ],
      "settings": {
        "foreground": "#C98A8E"
      }
    },
    {
      "name": "Colon",
      "scope": [
        "punctuation.separator.colon",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#C98A8E"
      }
    },
    {
      "name": "Parentheses - explicit white",
      "scope": [
        "punctuation.section.parens",
        "punctuation.section.parens.begin",
        "punctuation.section.parens.end",
        "meta.group"
      ],
      "settings": {
        "foreground": "#C98A8E"
      }
    },
    {
      "name": "Braces - explicit white",
      "scope": [
        "punctuation.section.braces",
        "punctuation.section.braces.begin",
        "punctuation.section.braces.end",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#C98A8E"
      }
    },
    {
      "name": "Operator and Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.section.brackets",
        "punctuation.section.brackets.begin",
        "punctuation.section.brackets.end",
        "punctuation.terminator",
        "punctuation.definition.parameters",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation",
        "punctuation.definition",
        "punctuation.section",
        "meta.bracket"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "punctuation.definition.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#10a37f"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.export",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#519BD9"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#10a37f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    }
  ]
}
