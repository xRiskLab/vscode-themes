{
  "name": "baseline-theme",
  "displayName": "Baseline Theme",
  "description": "VS Code theme based on Obsidian Baseline theme - sleek, minimal, and customizable with dark and light variants",
  "version": "1.0.1",
  "publisher": "xRiskLab",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "baseline",
    "obsidian",
    "theme",
    "dark",
    "light",
    "minimal"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Baseline Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/baseline-dark.json"
      },
      {
        "label": "Baseline Light",
        "uiTheme": "vs",
        "path": "./themes/baseline-light.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aaaaalexis/obsidian-baseline"
  },
  "license": "MIT",
  "extensionKind": [
    "ui"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  }
}

